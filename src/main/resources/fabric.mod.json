{
  "schemaVersion": 1,
  "id": "utility-structures",
  "version": "${version}",

  "name": "Utility structires",
  "description": "A minecraft mod that allows to add to custom structures some useful effects",
  "authors": [
    "Juarrin"
  ],
  "contact": {
    "homepage": "https://github.com/John-Development",
    "sources": "https://github.com/John-Development/utility-structures"
  },

  "license": "GPL-3.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.utilityStructures.UtilityStructures"
    ]
  },
  "mixins": [
    "utilityStructures.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
